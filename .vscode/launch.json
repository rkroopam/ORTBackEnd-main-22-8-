{
	"version": "0.0.1",
	"configurations": [{
		"type": "node",
		"request": "launch",
		"name": "RUN Dev API",
		"sourceMaps": true,
		"program": "${workspaceFolder}/build/app.js",
		"runtimeExecutable": "${workspaceRoot}/node_modules/.bin/babel-node",
		"cwd": "${workspaceFolder}",
		"port": 5858,
		"console": "integratedTerminal",
		"env": {
			"NODE_ENV": "development"
		},
		"runtimeArgs": ["--inspect=5858"],
		"timeout": 20000
	}]
}